(load "graphic-styles.scm")
(load "tools/util.scm")

(define (draw)
  (style-apply 'default-font)
  (define *height* 27)
  (rectstroke (make-rect 0 0 72 *height*))
  (rectstroke (make-rect 72 0 72 *height*))
  (moveto 144 *height*)
  (lineto 177 *height*)
  (moveto 183 *height*)
  (lineto 216 *height*)
  (moveto 144 0)
  (lineto 177 0)
  (moveto 183 0)
  (lineto 216 0)
  (moveto 174 -5);begin lines
  (lineto 180 5)
  (moveto 180 -5)
  (lineto 186 5)
  (moveto 174 (- *height* 5))
  (lineto 180 (+ *height* 5))
  (moveto 180 (- *height* 5))
  (lineto 186 (+ *height* 5))
  (stroke)
  (cshow 36 (-(/ *height* 2) 2)"type id")
  (cshow 108 (-(/ *height* 2) 2)"instance id")
  (cshow 180 (-(/ *height* 2) 2)"payload"))
